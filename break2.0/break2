for a in range(int(150  1/5)):
    for b in range(int(150  1/5)):
        for c in range(int(150  1/5)):
            for d in range(int(150  1/5)):
                for e in range(int(150  1/5)):
                    if a  5 + b  5 + c  5 + d  5 == e  5:
                        print(f'a = {a}, b = {b}, c = {c}, d = {d}, e = {e}')
